{
    "name": "app/tenant-bundle",
    "type": "symfony-bundle",
    "require": {
        "php": ">=7.4",
        "ext-openssl": "*",
        "symfony/framework-bundle": "^4.0 || ^5.0",
        "symfony/yaml": "^4.0 || ^5.0",
        "symfony/console": "^4.0 || ^5.0",
        "symfony/dependency-injection": "^4.0 || ^5.0",
        "symfony/process": "^4.0 || ^5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~7.5"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "authors": [
        {
            "name": "Vlad Buhanevych",
            "email": "dragonqos@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": { "App\\TenantBundle\\": "" }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\TenantBundle\\Tests\\": "Tests/"
        }
    }
}
