{
    "name": "app/tenant-bundle",
    "type": "symfony-bundle",
    "require": {
        "php": ">=7.4 || >=8.0",
        "ext-openssl": "*",
        "symfony/dependency-injection": "^5.0",
        "symfony/yaml": "^5.0",
        "symfony/console": "^5.0",
        "symfony/process": "^5.0",
        "symfony/annotations-pack": "^1.0",
        "symfony/event-dispatcher": "^5.0",
        "symfony/options-resolver": "^5.0",
        "symfony/orm-pack": "^1.0",
        "symfony/validator": "^5.0",
        "symfony/serializer": "^5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~7.5"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "authors": [
        {
            "name": "Vlad Bukhanevych",
            "email": "dragonqos@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": { "App\\TenantBundle\\": "" }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\TenantBundle\\Tests\\": "Tests/"
        }
    }
}
